{
  "name": "logos-notes-exporter-monorepo",
  "version": "1.0.9",
  "description": "Logos Notes Exporter - CLI and Electron applications with shared core",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "build": "npm run build:core && npm run build:cli && npm run build:electron",
    "build:core": "npm run build --workspace=@logos-notes-exporter/core",
    "build:cli": "npm run build --workspace=@logos-notes-exporter/cli",
    "build:electron": "npm run build --workspace=logos-notes-exporter-electron",
    "dev:cli": "npm run dev --workspace=@logos-notes-exporter/cli",
    "dev:electron": "npm run start --workspace=logos-notes-exporter-electron",
    "start": "npm run dev:electron",
    "test:electron": "npm run start --workspace=logos-notes-exporter-electron",
    "test:cli": "npm run dev --workspace=@logos-notes-exporter/cli",
    "clean": "npm run clean --workspaces --if-present"
  },
  "keywords": [
    "logos",
    "bible",
    "notes",
    "markdown",
    "export",
    "converter",
    "cli",
    "electron",
    "typescript",
    "bun"
  ],
  "author": "Logos Notes Exporter",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agape-apps/logos-notes-exporter.git"
  },
  "bugs": {
    "url": "https://github.com/agape-apps/logos-notes-exporter/issues"
  },
  "homepage": "https://github.com/agape-apps/logos-notes-exporter#readme",
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
